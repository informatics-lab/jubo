    "keywords": [
        "jupyter",
        "jupyterlab",
        "jupyterlab-extension",
        "jubo",
        "bokeh"
    ],
    "homepage": "https://github.com/met-office-lab/jubo",
    "bugs": {
        "url": "https://github.com/met-office-lab/jubo/issues"
    },
    "author": "theo.mccaie",
    "files": [
        "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
